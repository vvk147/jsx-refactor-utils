{
  "name": "jsx-refactor-utils",
  "version": "1.0.0",
  "description": "A utility library providing automated refactoring suggestions for JSX code to improve codebase maintainability and readability.",
  "main": "index.js",
  "scripts": {
    "lint": "eslint src --fix",
    "format": "prettier --write '**/*.{js,jsx}'",
    "test": "jest",
    "build": "babel src --out-dir lib",
    "prepublishOnly": "npm run lint && npm run format && npm run test && npm run build",
    "version": "npm version patch && git push --follow-tags",
    "postversion": "npm publish",
    "generate-docs": "jsdoc -c jsdoc.json -d docs",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@commitlint/cli": "^18.2.0",
    "@commitlint/config-conventional": "^18.1.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "eslint-plugin-n": "^16.3.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.2",
    "lint-staged": "^15.0.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.jsx": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.2",
    "@babel/types": "^7.23.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
